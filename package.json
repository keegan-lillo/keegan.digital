{
  "name": "keegan.digital",
  "version": "1.0.0",
  "description": "Keegan Digital Website",
  "author": "Keegan Lillo <keegan@lillo.me>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "node-version": "node --version",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "deploy": "clean && lint && build",
    "lint": "run-p --aggregate-output --continue-on-error lint:*",
    "lint:css": "stylelint '**/*.{scss,css}'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier . --check",
    "serve": "gatsby build && serve ./public",
    "start": "gatsby develop",
    "type-check": "tsc --noEmit"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "dependencies": {
    "@svgr/webpack": "^5.4.0",
    "clsx": "^1.1.1",
    "gatsby": "^2.24.53",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "^4.3.12",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-ts": "^2.7.1",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-remark": "^2.8.32",
    "graphql": "^14.7.0",
    "netlify-cms-app": "^2.12.20",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@keegan-lillo/eslint-config-typescript-react": "^0.4.0",
    "@keegan-lillo/prettier-config": "^0.4.0",
    "@keegan-lillo/stylelint-config-scss": "^0.4.0",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2",
    "stylelint": "^13.7.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "*.{scss,css}": "stylelint",
    "*": "prettier --check"
  }
}
