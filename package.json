{
  "name": "keegan.digital",
  "version": "1.0.0",
  "description": "Keegan Digital Website",
  "author": "Keegan Lillo <keegan@lillo.me>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "deploy": "clean && lint && build",
    "lint": "npm-run-all --aggregate-output --parallel lint:*",
    "lint:css": "stylelint '**/*.{scss,css}'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier . --check",
    "serve": "gatsby build && serve ./public",
    "start": "gatsby develop",
    "type-check": "tsc --noEmit"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "dependencies": {
    "@svgr/webpack": "5.4.0",
    "clsx": "1.1.0",
    "gatsby": "2.21.22",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-sass": "2.3.1",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-ts": "2.7.1",
    "gatsby-plugin-typescript-checker": "1.1.1",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-transformer-remark": "2.8.7",
    "graphql": "14.6.0",
    "node-sass": "4.14.1",
    "normalize.css": "8.0.1",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "ts-node": "8.10.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-typescript": "7.9.0",
    "@babel/register": "7.9.0",
    "@keegan-lillo/eslint-config-typescript-react": "0.1.1",
    "@keegan-lillo/prettier-config": "0.1.1",
    "@keegan-lillo/stylelint-config-scss": "0.1.1",
    "@types/node": "13.13.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "5.0.15",
    "eslint": "7.0.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "npm-run-all": "4.1.5",
    "serve": "11.3.0",
    "stylelint": "13.3.3",
    "typescript": "3.8.3",
    "webpack": "4.43.0"
  },
  "resolutions": {
    "eslint": ">=7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "*.{scss,css}": "stylelint",
    "*": "prettier --check"
  }
}
