{
  "name": "keegan.digital",
  "version": "1.0.0",
  "description": "Keegan Digital Website",
  "author": "Keegan Lillo <keegan@lillo.me>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "deploy": "clean && lint && build",
    "lint": "npm-run-all --aggregate-output --parallel lint:*",
    "lint:css": "stylelint '**/*.{scss,css}'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier . --check",
    "serve": "gatsby build && serve ./public",
    "start": "gatsby develop",
    "type-check": "tsc --noEmit"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "dependencies": {
    "@svgr/webpack": "^5.4.0",
    "clsx": "^1.1.1",
    "gatsby": "^2.24.51",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-ts": "^2.7.1",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^2.3.26",
    "gatsby-transformer-remark": "^2.8.30",
    "graphql": "^14.7.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@keegan-lillo/eslint-config-typescript-react": "^0.2.0",
    "@keegan-lillo/prettier-config": "^0.2.0",
    "@keegan-lillo/stylelint-config-scss": "^0.2.0",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-typescript-sort-keys": "^1.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2",
    "stylelint": "^13.6.1",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  },
  "resolutions": {
    "eslint": ">=7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "*.{scss,css}": "stylelint",
    "*": "prettier --check"
  }
}
