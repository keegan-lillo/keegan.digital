{
  "name": "keegan.digital",
  "version": "1.0.0",
  "description": "Keegan Digital Website",
  "author": "Keegan Lillo <keegan@lillo.me>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "node-version": "node --version",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "lint": "run-p --aggregate-output --continue-on-error lint:*",
    "lint:css": "stylelint '**/*.{scss,css}'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier . --check",
    "serve": "gatsby build && serve ./public",
    "start": "gatsby develop",
    "local-cms": "npx netlify-cms-proxy-server",
    "type-check": "tsc --noEmit"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "dependencies": {
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "gatsby": "^3.4.1",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-image": "^1.4.0",
    "gatsby-plugin-netlify": "^3.4.0",
    "gatsby-plugin-netlify-cms": "^5.4.1",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sass": "^4.4.0",
    "gatsby-plugin-sharp": "^3.4.1",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "gatsby-remark-copy-linked-files": "^4.1.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^5.1.0",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-transformer-remark": "^4.1.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "graphql": "^15.5.0",
    "netlify-cms-app": "^2.15.2",
    "netlify-plugin-gatsby-cache": "^0.3.2",
    "node-sass": "^5.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "web-api-hooks": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@keegan-lillo/eslint-config-typescript-react": "^0.5.0",
    "@keegan-lillo/prettier-config": "^0.5.0",
    "@keegan-lillo/stylelint-config-scss": "^0.5.0",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2",
    "stylelint": "^13.13.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "*.{scss,css}": "stylelint",
    "*": "prettier --check"
  }
}
